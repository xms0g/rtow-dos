all:
	MAKE.EXE clean
	TASM.EXE /ml VGA.ASM
	TCC.EXE -c -oVEC3.OBJ VEC3.C
	TCC.EXE -c -oMATH.OBJ MATH.C
	TCC.EXE -c -oCOLOR.OBJ COLOR.C
	TCC.EXE -c -oSPHERE.OBJ SPHERE.CPP 
	TCC.EXE -c -oSCENE.OBJ SCENE.CPP
	TCC.EXE -c -oCAMERA.OBJ CAMERA.CPP
	TCC.EXE -c -oRAY.OBJ RAY.C
	TCC.EXE RT86.CPP VGA.OBJ VEC3.OBJ MATH.OBJ COLOR.OBJ SPHERE.OBJ SCENE.OBJ CAMERA.OBJ RAY.OBJ
clean:
	DEL RT86.EXE
	DEL RT86.OBJ
	DEL VGA.OBJ
	DEL VEC3.OBJ
	DEL MATH.OBJ
	DEL COLOR.OBJ
	DEL SPHERE.OBJ
	DEL SCENE.OBJ
	DEL CAMERA.OBJ
	DEL RAY.OBJ